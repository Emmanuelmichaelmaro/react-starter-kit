{
    "env": {
        "commonjs": true,
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": [
        // "airbnb",
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": ["prettier", "react"],
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
